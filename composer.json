{
  "name": "originphp/framework",
  "description": "The OriginPHP framework",
  "type": "library",
  "keywords": [
    "originPHP",
    "framework",
    "mvc",
    "easy",
    "simple",
    "convention over configuration",
    "orm",
    "docker",
    "rapid"
  ],
  "homepage": "https://www.originphp.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamiel Sharief",
      "email": "js@originphp.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Origin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Origin\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2.0",
    "originphp/defer" : "^1.0",
    "originphp/configurable" : "^1.0",
    "originphp/inflector" : "^1.0",
    "originphp/security" : "^1.0",
    "originphp/cache" : "^1.0",
    "originphp/email" : "^1.0",
    "originphp/html" : "^1.0",
    "originphp/xml" : "^1.0",
    "originphp/log" : "^1.0",
    "originphp/validation" : "^0.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-intl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "suggest": {
    "ext-redis": "Redis",
    "ext-imap": "Imap",
    "ext-mailparse": "Mailparse"
  }
}
<?xml version="1.0" encoding="UTF-8"?>
<phpunit
    colors="true"
    processIsolation="false"
    stopOnFailure="false"
    bootstrap="./origin/src/bootstrap.php"
    backupGlobals="true"
    >
    <filter>
        <whitelist>
            <directory suffix=".php">./origin/src/</directory>
        </whitelist>
    </filter>
</phpunit>
